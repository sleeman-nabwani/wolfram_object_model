default;\\n};\\n\\nclass ConcreteBase {\\npublic:\\n    virtual void \
concreteMethod() {}\\n    virtual ~ConcreteBase() {}\\n    \\nprivate:\\n    \
int concreteData;\\n};\\n\\nclass InterfaceImpl1 : public Interface1, public \
